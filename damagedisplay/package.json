{
  "name": "damagedisplay",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.6.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.20.50",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "concurrently": "^5.3.0",
    "electron-is-dev": "^1.2.0",
    "electronmon": "^1.1.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "prettier": "^2.6.2",
    "prettier-eslint": "^13.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^3.4.1",
    "sass": "^1.51.0",
    "typescript": "^4.6.4",
    "wait-on": "^5.3.0"
  },
  "devDependencies": {
    "electron": "^9.4.4",
    "electron-builder": "^22.14.13"
  },
  "scripts": {
    "start": "concurrently \"npm run react-serve\" \"wait-on http://localhost:3000 && npm run electron-start \"",
    "build": "npm run react-build && npm run electron-build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "release": "npm run react-build && npm run electron-release",
    "electron-start": "electronmon .",
    "electron-build": "electron-builder",
    "electron-release": "electron-builder --publish=always",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-serve": "SET BROWSER=none&&npm run react-start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "public/electron.js",
  "homepage": "./",
  "description": "Displays damage data captured by LostArkPacketCapture",
  "author": "Marsunpaisti"
}
